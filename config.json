{
    "data": {
        "train": {
            "captions": "data/captions/norm_train.lc.norm.tok.en",
            "features": "data/features/train-resnet50-avgpool.npy",
            "links": "data/links/train.txt"
        },
        "val": {
            "captions": "data/captions/norm_val.lc.norm.tok.en",
            "features": "data/features/val-resnet50-avgpool.npy",
            "links": "data/links/val.txt"
        },
        "images": "data/images",
        "vocab": "data/flickr_vocab.en"
    },
    "seed": 1561478941,
    "max_epoch": 3,
    "logging": {
        "activate": true,
        "output_folder": "test"
    },
    "cuda": {
        "ngpu": 1,
        "device": "cuda:0"
    },
    "sampler": {
        "train": {
            "batch_size": 32,
            "max_len": 20
        },
        "val": {
            "batch_size": 32,
            "max_len": 20
        },
        "beam": {
            "batch_size": 32,
            "drop_last": false
        }
    },
    "iterator": {
        "train": {
            "pin_memory": false,
            "num_workers": 0
        },
        "val": {
            "pin_memory": false,
            "num_workers": 0
        },
        "beam": {
            "pin_memory": false,
            "num_workers": 0
        }
    },
    "model": {
        "emb_dim": 128,
        "dec_dim": 256,
        "gradient_weight": 10,
        "feature_size": 2048,
        "optimizers": {
            "lr": 0.002,
            "weight_decay": 1e-05,
            "betas": {
                "min": 0.5,
                "max": 0.999
            }
        },
        "generator": {
            "dropout": 0.2,
            "dropout_type": "local",
            "train_iteration": 5,
            "dec_init_type": "random",
            "att_activ": "tanh"
        },
        "discriminator": {
            "dec_init_type": "zero",
            "att_activ": "relu"
        }
    },
    "beam_search": {
        "beam_size": 5,
        "max_len": 20
    }
}